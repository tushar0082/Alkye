<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Define the shape as a rectangle -->
    <solid android:color="@android:color/transparent"/> <!-- Transparent background -->

    <!-- Set the stroke (border) -->
    <stroke
        android:width="1dp"
        android:color="#000000"/> <!-- Black border -->

    <!-- Define the corner radius for rounded corners -->
    <corners android:radius="16dp"/> <!-- Adjust the radius as needed -->
</shape>
